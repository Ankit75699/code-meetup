<% include adminheader %>
<h1>add events here</h1>
<div class="container  my-4">
  <h2><%= msg %></h2>
  <form method="post" enctype="multipart/form-data" action="">
    <div class="form-group">
      <label for="exampleInputtitle1">Title</label>
      <input type="text" name="title" class="form-control" id="exampleInputtitle1">
    </div>
    <div class="form-group">
      <label for="exampleInputdate1">Date</label>
      <input type="date" name="date" class="form-control" id="exampleInputdate1">
    </div>
    <div class="form-group">
      <label for="appt">Select event start time:</label>
      <input type="time" id="appt" name="appt">
      <label for="appt2">Select event ending time:</label>
      <input type="time" id="appt2" name="appt2">
    </div>
    <div class="form-group">
      <label for="exampleInputfile2">Date</label>
      <input type="file" name="file2" class="form-control" id="exampleInputfile2">
    </div>
    <div class="form-group">
      <label for="exampleInputArticle1">Event Info</label>
      <textarea name="Article" class="form-control" id="exampleInputArticle"></textarea>
    </div>
    <div>
      <button type="submit" class="btn btn-primary">Creat Account</button>
    </div>
  </form>
</div>