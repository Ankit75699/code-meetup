<% include adminheader %>
<div class="container  my-4">
  <section>
    <form method="post" action="" enctype="multipart/form-data">
      <h3><%= msg %></h3>
      <div class="form-group">
        <label>Course Name</label>
        <input type="text" class="form-control" name="coursenm" placeholder="Enter Course Name" />
      </div>

      <div class="form-group">
        <label>Faculty Name</label>
        <input type="text" class="form-control" name="facultynm" placeholder="Enter Faculty Name" />
      </div>

      <div class="form-group">
        <label>Course Image</label>
        <input type="file" name="file1" class="form-control" />
      </div>

      <div class="form-group">
        <label>Course Description</label>
        <textarea name="text" class="form-control"></textarea>
      </div>

      <div class="form-group">
        <button type="submit" class="btn btn-primary">Creat Account</button>
      </div>

    </form>
  </section>
</div>
<% include script%>
<% include footer %>